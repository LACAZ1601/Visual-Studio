algoritmo "semnome"
var
a, b, c: real
inicio
escreval("Insira o valor de A")
leia(a)
escreval("Insira o valor de B")
leia(b)

escreval("----------")
escreva("O valor de A é:")
escreval(a)
escreva("O valor de B é:")
escreval(b)

c<-a
a<-b
b<-c

escreval("----------")
escreva("Agora o valor de A é:")
escreval(a)
escreva("Agora o valor de B é:")
escreval(b)
fimalgoritmo
